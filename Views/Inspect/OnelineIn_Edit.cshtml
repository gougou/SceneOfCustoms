
<form id="OrderFrom" class="OrderFrom" method="post">
    <div id="" style="margin-bottom:20px" class="easyui-panel" title="订单主信息"
         data-options="collapsible:true">
        <input type="hidden" name="ID" />
        <input type="hidden" name="type" value="02" />
        <table>

            <tr>
                <td>
                    <label for="FWONO">FWO订单号:</label>
                    <input class="easyui-textbox" type="text" name="FWONO" disabled="disabled" />
                </td>
                <td>
                    <label for="FOONOBJ">FO号:</label>
                    <input class="easyui-textbox" type="text" name="FOONOBJ" disabled="disabled" />
                </td>
                <td>
                    <label for="TOTALNO">总单号:</label>
                    <input class="easyui-textbox" type="text" name="TOTALNO" disabled="disabled" />
                </td>
                <td>
                    <label for="DIVIDENO">分单号:</label>
                    <input class="easyui-textbox" type="text" name="DIVIDENO" disabled="disabled" />
                </td>
            </tr>

            <tr>
                <td>
                    <label for="text">万达号:</label>
                    <input class="easyui-textbox" type="text" name="text" disabled="disabled" />
                </td>
                <td>
                    <label for="MANIFEST">载货清单号:</label>
                    <input class="easyui-textbox" type="text" name="MANIFEST" disabled="disabled" />
                </td>
                <td>
                    <div style="text-align:center">
                        散货:
                        <input class="" type="radio" name="" disabled="disabled" />
                        整箱:
                        <input class="" type="radio" name="" disabled="disabled" />
                        拼箱:
                        <input class="" type="radio" name="" disabled="disabled" />
                    </div>
                </td>

                <td>
                    <div style="text-align:center">
                        木质包装:
                        <input class="" type="checkbox" name="" disabled="disabled" />
                        通关单否:
                        <input class="" type="checkbox" name="" disabled="disabled" />
                        法检标志:
                        <input class="" type="checkbox" name="" disabled="disabled" />
                    </div>
                </td>
            </tr>


            <tr>
                <td>
                    <label for="CODE">业务编号:</label>
                    <input class="easyui-textbox" type="text" name="CODE" disabled="disabled" />
                </td>
                <td>
                    <label for="">收货人:</label>
                    <input class="easyui-textbox" type="text" name="SFGOODSUNIT" disabled="disabled" />
                </td>
                <td>
                    <label for="CONTRACTNO"> 合同/发票号:</label>
                    <input class="easyui-textbox" type="text" name="CONTRACTNO" disabled="disabled" />
                </td>
            </tr>
            <tr>
                <td>
                    <label for="GOODSNUM"> 总件数:</label>
                    <input class="easyui-textbox" type="text" name="GOODSNUM" disabled="disabled" />
                </td>
                <td>
                    <label for="GOODSWEIGHT">总重量:</label>
                    <input class="easyui-textbox" type="text" name="GOODSWEIGHT" disabled="disabled" />
                </td>
                <td>
                    <label for="FIRSTLADINGBILLNO">一程提运单号:</label>
                    <input class="easyui-textbox" type="text" name="FIRSTLADINGBILLNO" disabled="disabled" />
                </td>
                <td>
                    <label for="SECONDLADINGBILLNO">二程提运单号:</label>
                    <input class="easyui-textbox" type="text" name="SECONDLADINGBILLNO" disabled="disabled" />
                </td>
            </tr>



        </table>
    </div>

    <div class="easyui-panel" style="margin-bottom:15px" title="现场关务"
         data-options="collapsible:true">
        <table id="scene">
            <tr>
                <td>
                    <label for="BAOJIANTIME"> 报检时间:</label>
                    <input class="easyui-textbox time_input INSPSTATUS" datatype="BAOJIAN" id="BAOJIANTIME" name="BAOJIANTIME" editable="false" />
                    @*<a href="javascript:void(0)" datatype="BAOJIAN" class="easyui-linkbutton fillingData INSPSTATUS"><i class="icon-time"></i></a>*@
                </td>
                <td>
                    <label for="BAOJIANUSERNAME">报检人:</label>
                    <input class="easyui-textbox " type="text" id="BAOJIANUSERNAME" name="BAOJIANUSERNAME" disabled="disabled" />
                </td>
                <td>
                    <label for="BAOJIANFANGXINGTIME"> 报检放行时间:</label>
                    <input class="easyui-textbox time_input" datatype="BAOJIANFANGXING" id="BAOJIANFANGXINGTIME" name="BAOJIANFANGXINGTIME" editable="false" />
                    @*<a href="javascript:void(0)" datatype="BAOJIANFANGXING" class="easyui-linkbutton fillingData"><i class="icon-time"></i></a>*@
                </td>
                <td>
                    <label for="BAOJIANFANGXINGUSERNAME">报检放行人:</label>
                    <input class="easyui-textbox " type="text" id="BAOJIANFANGXINGUSERNAME" name="BAOJIANFANGXINGUSERNAME" disabled="disabled" />
                </td>
            </tr>

            <tr>
                <td>
                    <label for="SHANDANKAISHITIME"> 删单时间:</label>
                    <input class="easyui-textbox time_input" datatype="SHANDANKAISHI" id="SHANDANKAISHITIME" name="SHANDANKAISHITIME" editable="false" />
                    @*<a href="javascript:void(0)" datatype="SHANDANKAISHI" class="easyui-linkbutton fillingData"><i class="icon-time"></i></a>*@
                </td>
                <td>
                    <label for="SHANDANKAISHIUSERNAME">删单人:</label>
                    <input class="easyui-textbox " type="text" id="SHANDANKAISHIUSERNAME" name="SHANDANKAISHIUSERNAME" disabled="disabled" />
                </td>
                <td>
                    <label for="GAIDANKAISHITIME"> 改单时间:</label>
                    <input class="easyui-textbox time_input" datatype="GAIDANKAISHI" id="GAIDANKAISHITIME" name="GAIDANKAISHITIME" editable="false" />
                    @*<a href="javascript:void(0)" datatype="GAIDANKAISHI" class="easyui-linkbutton fillingData"><i class="icon-time"></i></a>*@
                </td>
                <td>
                    <label for="GAIDANKAISHIUSERNAME">改单人:</label>
                    <input class="easyui-textbox " type="text" id="GAIDANKAISHIUSERNAME" name="GAIDANKAISHIUSERNAME" disabled="disabled" />
                </td>
            </tr>

            <tr>
                <td>
                    <label for="CHAYANZHILINGXIAFATIME"> 查验时间:</label>
                    <input class="easyui-textbox time_input INSPSTATUS" datatype="CHAYANZHILINGXIAFA" id="CHAYANZHILINGXIAFATIME" name="CHAYANZHILINGXIAFATIME" editable="false" />
                    @*<a href="javascript:void(0)" datatype="CHAYANZHILINGXIAFA" class="easyui-linkbutton fillingData INSPSTATUS"><i class="icon-time"></i></a>*@
                </td>
                <td>
                    <label for="CHAYANZHILINGXIAFAUSERNAME">查验人:</label>
                    <input class="easyui-textbox " type="text" id="CHAYANZHILINGXIAFAUSERNAME" name="CHAYANZHILINGXIAFAUSERNAME" disabled="disabled" />
                </td>
                <td>
                    <label for="CHAYANFANGXINGTIME"> 查验放行时间:</label>
                    <input class="easyui-textbox time_input" datatype="CHAYANFANGXING" id="CHAYANFANGXINGTIME" name="CHAYANFANGXINGTIME" editable="false" />
                    @*<a href="javascript:void(0)" datatype="CHAYANFANGXING" class="easyui-linkbutton fillingData"><i class="icon-time"></i></a>*@
                </td>
                <td>
                    <label for="CHAYANFANGXINGUSERNAME">查验放行人:</label>
                    <input class="easyui-textbox " type="text" id="CHAYANFANGXINGUSERNAME" name="CHAYANFANGXINGUSERNAME" disabled="disabled" />
                </td>
            </tr>

            <tr>
                <td>
                    <label for="XUNZHENGTIME">熏蒸时间:</label>
                    <input class="easyui-textbox time_input INSPSTATUS" datatype="XUNZHENG" id="XUNZHENGTIME" name="XUNZHENGTIME" editable="false" />
                    @*<a href="javascript:void(0)" datatype="XUNZHENG" class="easyui-linkbutton fillingData INSPSTATUS"><i class="icon-time"></i></a>*@
                </td>
                <td>
                    <label for="XUNZHENGUSERNAME">熏蒸人:</label>
                    <input class="easyui-textbox " type="text" id="XUNZHENGUSERNAME" name="XUNZHENGUSERNAME" disabled="disabled" />
                </td>

                <td style="text-align:left">
                    <label for="IFXUNZHENG">是否熏蒸:</label>
                    <input class="" id="IFXUNZHENG" type="checkbox" name="IFXUNZHENG" value="1" />
                </td>
                <td>
                    <label for="XUNZHENGDESC">熏蒸备注:</label>
                    <input class="easyui-textbox" type="text" name="XUNZHENGDESC">
                </td>
            </tr>

            <tr>
                <td style="text-align:left">
                    <label for="shifjiancha">是否查检:</label>
                    <input class="" id="shifjiancha" type="checkbox" name="IFCHAYAN" value="1" />
                </td>
                <td>
                    <label for="">查验次数:</label>
                    <input class="easyui-numberspinner" name="CHAYANTIMES"
                           data-options="min:0">
                </td>
                <td>
                    <label for="">查验备注:</label>
                    <input class="easyui-textbox" type="text" name="CHAYANREMARK" />
                </td>
                <td>
                    <label>查验类别:</label>
                    <select class="easyui-combobox" name="CHAYANTYPE">
                        <option value="">请选择</option>
                        <option value="集装箱">集装箱</option>
                        <option value="木托">木托</option>
                        <option value="检疫物">检疫物</option>
                    </select>
                </td>
            </tr>

            <tr>
                <td style="text-align:left">
                    <label for="IFSHANDAN">是否删单:</label>
                    <input class="" id="IFSHANDAN" type="checkbox" name="IFSHANDAN" value="1" />
                </td>
                <td>
                    <label for="SHANDANTOTAL">删单次数:</label>
                    <input class="easyui-numberspinner" name="SHANDANTOTAL"
                           data-options="min:0">
                </td>
                <td>
                    <label for="SHANDANDESC">删单备注:</label>
                    <input class="easyui-textbox" name="SHANDANDESC">
                </td>

                <td>
                    <label for="INSPSTATUS">报检状态:</label>
                    @*<select class="easyui-combobox" name="INSPSTATUS">
                            <option value="">请选择</option>
                            <option value="报检">报检</option>
                            <option value="木托">查验</option>
                            <option value="熏蒸">熏蒸</option>
                        </select>*@

                    <input class="easyui-textbox" name="INSPSTATUS" id="INSPSTATUS">
                </td>
            </tr>

            <tr>
                <td style="text-align:left">
                    <label for="IFGAIDAN">是否改单:</label>
                    <input class="" id="IFGAIDAN" type="checkbox" name="IFGAIDAN" value="1" />
                </td>
                <td>
                    <label for="GAIDANTOTAL">改单次数:</label>
                    <input class="easyui-numberspinner" name="GAIDANTOTAL"
                           data-options="min:0">
                </td>
                <td>
                    <label for="GAIDANDESC">改单备注:</label>
                    <input class="easyui-textbox" name="GAIDANDESC">
                </td>
            </tr>

        </table>
    </div>

    <div style="text-align:center;padding:5px 0">
        <a class="btn btn-primary" href="javascript:void(0)" onclick="TongguandanInfo()">
            <i class="fa fa-file-text-o" aria-hidden="true"></i> 通关单信息
        </a>
        <a class="btn btn-primary" href="javascript:void(0)" onclick="JizhuangxiangInfo()">
            <i class="fa fa-suitcase" aria-hidden="true"></i> 集装箱信息
        </a>
        <a class="btn btn-primary" href="javascript:void(0)" onclick="submitForm()">
            <i class="fa fa-floppy-o"></i> 保存
        </a>
        <a class="btn btn-primary" href="javascript:void(0)" onclick="javascript: history.back(-1);">
            <i class="icon-reply"></i> 返回
        </a>
    </div>
</form>

<script type="text/javascript">
    Edit();
</script>
