                    <input class="easyui-combobox" name="CUSTOMDISTRICTCODE" data-options="
					url: '/Order/Get_SBGQ',
					method: 'get',
					valueField: 'CODE',
					textField: 'NAME',
					panelWidth: 200,
					panelHeight: 'auto',
					formatter: formatItem,
					label: 'CUSTOMDISTRICTCODE:',
                    panelHeight:'200',
					labelPosition: 'top'
					">
                </td>
            </tr>
            <tr>
                <td>
                    <label for="ARRIVEDNO">运抵编号:</label>
                    <input class="easyui-textbox" type="text" name="ARRIVEDNO" disabled="disabled" />
