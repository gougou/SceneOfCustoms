                <td>
                    <label for="GAIDANWANCHENGTIME"> 改单完成时间:</label>
                    <input class="easyui-textbox time_input" datatype="GAIDANWANCHENG" id="GAIDANWANCHENGTIME" name="GAIDANWANCHENGTIME" editable="false" />
                    @*<a href="javascript:void(0)" datatype="GAIDANWANCHENG" class="easyui-linkbutton fillingData"><i class="icon-time"></i></a>*@
                </td>
                <td>
                    <label for="GAIDANWANCHENGUSERNAME">改单完成人:</label>
                    <input class="easyui-textbox " type="text" id="GAIDANWANCHENGUSERNAME" name="GAIDANWANCHENGUSERNAME" disabled="disabled" />
                </td>
            </tr>
            <tr>
                <td>
                    <label for="CHAYANSTARTTIME"> 查验起始时间:</label>
                    <input class="easyui-textbox time_input DECLSTATUS" datatype="CHAYANSTART" id="CHAYANSTARTTIME" name="CHAYANSTARTTIME" editable="false" />
