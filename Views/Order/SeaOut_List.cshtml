<div class="ibox-content">
    <div class="table-responsive">
        <table id="datagrid" class="easyui-datagrid" title="海运出口列表" style="height:auto;min-height:500px;max-height:800px"
               data-options=" rownumbers:true,
                                   url: '/Order/GetData',
                                   queryParams:{BUSITYPE:20},
                                   dataType : 'json',
                                   method:'get',
                                   toolbar:'#tb',
                                   pageSize:20,
                                   pagination:true ">
            <thead data-options="frozen:true">
                <tr>
                    <th data-options="field: 'ck', checkbox: true"></th>
                    <th data-options="field:'CUSNO',width:200,sortable:true,align:'center'">收货人</th>
                    <th data-options="field:'test',width:120,sortable:true,align:'center'">进舱编号</th>
                    <th data-options="field:'GOODSNUM',width:80,align:'center',sortable:true">总件数</th>
                    <th data-options="field:'GOODSWEIGHT',width:70,sortable:true,align:'center'">总重量</th>
                </tr>
            </thead>

            <thead>

                <tr>
                    <th data-options="field:'DECLSHEETNUM',width:80,align:'center',sortable:true">报关单张数</th>
                    <th data-options="field:'LAWCONDITION',width:80,sortable:true,align:'center'">是否法检</th>
                    <th data-options="field:'ASSOCIATEPEDECLNO',width:120,sortable:true,align:'center'">报关单号</th>
                    <th data-options="field:'LIDANTIME',width:100,align:'center'">理单时间</th>
                    <th data-options="field:'BAOGUANTIME',width:100,align:'center'">报关时间</th>
                    <th data-options="field:'SHIWUJIAFENGTIME',width:100,align:'center'">实物加封时间</th>
                    <th data-options="field:'ASSOCIATETRADEWAY',width:100,align:'center'">贸易方式</th>
                    <th data-options="field:'attr1',width:100,align:'center'">报关备注</th>
                    <th data-options="field:'attr1',width:100,align:'center'">单证齐全时间</th>
                    <th data-options="field:'SHIWUFANGXINGTIME',width:100,sortable:true,align:'center'">实物放行时间</th>
                    <th data-options="field:'attr1',width:100,sortable:true,align:'center'">实物放行状态</th>
                    <th data-options="field:'attr1',width:100,align:'center'">理单人</th>
                    <th data-options="field:'YWBH',width:100,sortable:true">业务编号</th>
                    <th data-options="field:'CONTRACTNO',width:100,sortable:true">合同/发票号</th>
                    <th data-options="field:'TURNPRENO',width:70,sortable:true">转关预录入号</th>
                    <th data-options="field:'attr1',width:100">作业单时间</th>
                    <th data-options="field:'SHIWUFANGXINGUSERNAME',width:100">实物放行人</th>
                    <th data-options="field:'attr1',width:100">关务状态</th>
                    <th data-options="field:'attr1',width:100">关务代码</th>
                    <th data-options="field:'attr1',width:100">通关状态</th>
                    <th data-options="field:'attr1',width:100">OPS接单人</th>
                    <th data-options="field:'attr1',width:100">OPS接单时间</th>
                    <th data-options="field:'attr1',width:70">OPS接单备注</th>
                    <th data-options="field:'attr1',width:100">海关通关状态</th>
                    <th data-options="field:'attr1',width:100">签收号</th>
                    <th data-options="field:'attr1',width:100">签收人</th>
                    <th data-options="field:'attr1',width:100">签收日期</th>
                </tr>



            </thead>
        </table>

        <div id="tb">
            <div class="tb_search">
                <form id="searchform" method="post">
                    <table>
                        <tr>
                            <td class="search_margin">
                                <select class="easyui-combobox" id="search_code" panelheight="auto" name="ordercode"></select>
                            </td>
                            <td>
                                <input class="easyui-textbox value" type="text" name="ordercode_value" />
                            </td>

                            <td class="search_margin">
                                <select class="easyui-combobox" id="search_operator" panelheight="auto" name="oprname"></select>
                            </td>
                            <td>
                                <input class="easyui-textbox value" type="text" name="oprname_value" />
                            </td>

                            <td class="search_margin">
                                <select class="easyui-combobox" panelheight="auto">
                                    <option>业务对象</option>
                                </select>
                            </td>
                            <td>
                                <select class="easyui-combobox combobox_width value" id="business_object" panelheight="auto" name="business_object"></select>
                            </td>

                            <td class="search_margin">
                                <select class="easyui-combobox" id="search_condition_date" panelheight="auto" name="orderdate"></select>
                            </td>
                            <td>
                                <input class="easyui-datebox value" name="startdate" placeholder="开始时间">
                            </td>
                            <td>
                                <input class="easyui-datebox value" name="stopdate" placeholder="结束时间">
                            </td>
                        </tr>

                        <tr>
                            <td class="search_margin">
                                <select class="easyui-combobox" panelheight="auto">
                                    <option>海关通关状态</option>
                                </select>
                            </td>
                            <td>
                                <select class="easyui-combobox value" id="customs_state" style="width:130px" panelheight="auto" name="customs_state"></select>
                            </td>

                            <td class="search_margin">
                                <select class="easyui-combobox" panelheight="auto">
                                    <option>通关单标志</option>
                                </select>
                            </td>
                            <td>
                                <select class="easyui-combobox combobox_width value is_check"  panelheight="auto" name="is_clearance"></select>
                            </td>
                        </tr>


                    </table>
                </form>
            </div>
            <div class="tb_but">
                <div class="but_left">
                    @*<a class="btn btn-primary btn-xs" href="javascript:void(0)"><i class="icon-search"></i> 搜索</a>
                    <a class="btn btn-primary btn-xs" href="javascript:void(0)"><i class="icon-refresh"></i> 重置</a>*@
                </div>
                <div class="but_right">
                    <a class="btn btn-primary btn-xs" href="javascript:void(0)" onclick="search_form()"><i class="icon-search"></i> 搜索</a>
                    <a class="btn btn-primary btn-xs" href="javascript:void(0)" onclick="reset_form()"><i class="icon-refresh"></i> 重置</a>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">


    $(function () {

        //双击事件
        $('#datagrid').datagrid({
            onDblClickCell: function (index, field, value) {
                var row = $('#datagrid').datagrid('getData').rows[index];
                if (row.ID != "") {
                    window.location.href = "/Order/SeaOut_Edit?ID=" + row.ID;
                }
            }
        });

    });



</script>


