<script>

    $('#Wumao_datagrid').datagrid({
        onDblClickCell: function (index, field, value) {
            var row = $('#Wumao_datagrid').datagrid('getData').rows[index];
            $('#mainWin').dialog({
                title: '添加',
                width: 800,
                height: 400,
                modal: true,
                href: '/Wumao/DataMatching_Edit?ID=' + row.ID,
            });
        }
    });

    $("#syncdata_wumao").click(function () {
        $('#mainWin').dialog({
            title: '添加',
            width: 800,
            height: 400,
            modal: true,
            href: '/Wumao/DataMatching_Edit',
        });
    });

</script>